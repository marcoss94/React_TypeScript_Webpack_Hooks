{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": 3.9,
                "useBuiltIns": "usage"
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        "transform-class-properties",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ]
    ]
}